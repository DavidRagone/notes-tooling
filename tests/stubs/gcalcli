#!/usr/bin/env bash
# Minimal gcalcli stub that prints TSV format matching real gcalcli output
if [[ "$1" == "agenda" ]]; then
  # Print header
  echo -e "start_date\tstart_time\tend_date\tend_time\ttitle\tdescription\tcalendar"
  # Print one sample event
  echo -e "2025-11-08\t10:00\t2025-11-08\t10:30\tWeekly Sync\tDiscuss roadmap\ttest@example.com"
  exit 0
fi
echo "gcalcli stub: unhandled args $*" >&2
exit 0

